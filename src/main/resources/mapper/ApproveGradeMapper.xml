  </select> 
  <update id="update" parameterType="com.jackluan.bigflag.domain.flag.component.dataobject.ApproveGradeDo" > 
    update approve_grade 
    <set > 
     	  <if test="score != null"> score = #{score},</if> 
        <if test="isDelete != null"> is_delete = #{isDelete},</if>
        modify_date = now()
    </set> 
       where id = #{id,jdbcType=BIGINT}  
  </update> 
</mapper> 
