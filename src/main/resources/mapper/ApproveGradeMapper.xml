  </sql> 
  <sql id="Base_Column_List" > 
    	id,score,is_delete,create_date,modify_date 
  </sql> 
  <select id="select" resultMap="BaseResultMap" parameterType="com.jackluan.bigflag.domain.flag.component.dataobject.ApproveGradeDo" > 
    select 
    <include refid="Base_Column_List" /> 
    from approve_grade 
      <include refid="Example_Where_Clause" /> 
  </select> 
  <delete id="delete" parameterType="com.jackluan.bigflag.domain.flag.component.dataobject.ApproveGradeDo" > 
    update approve_grade set  is_delete='Y'
      <include refid="Example_Where_Clause" /> 
  </delete> 
  <insert id="insert" parameterType="com.jackluan.bigflag.domain.flag.component.dataobject.ApproveGradeDo"  useGeneratedKeys="true" keyProperty="id">
    insert into approve_grade 
     ( score,is_delete,create_date,modify_date )
    values 
    ( #{score},'N',now(),now() )
  </insert> 
  <select id="count" parameterType="com.jackluan.bigflag.domain.flag.component.dataobject.ApproveGradeDo" resultType="java.lang.Integer" > 
